{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "rosewater": "#F5E0DC",
    "flamingo": "#F2CDCD",
    "pink": "#F5C2E7",
    "mauve": "#CBA6F7",
    "red": "#F38BA8",
    "maroon": "#EBA0AC",
    "peach": "#FAB387",
    "yellow": "#F9E2AF",
    "green": "#A6E3A1",
    "teal": "#94E2D5",
    "sky": "#89DCEB",
    "sapphire": "#74C7EC",
    "blue": "#89B4FA",
    "lavender": "#B4BEFE",
    "text": "#CDD6F4",
    "subtext1": "#BAC2DE",
    "subtext0": "#A6ADC8",
    "overlay2": "#9399B2",
    "overlay1": "#7F849C",
    "overlay0": "#6C7086",
    "surface2": "#585B70",
    "surface1": "#45475A",
    "surface0": "#313244",
    "base": "#1E1E2E",
    "mantle": "#181825",
    "crust": "#11111B",
    "true": "p:green",
    "false": "p:red",
    "time": "p:overlay1",
    "os-fg": "p:base",
    "os-bg": "p:overlay1",
    "root-fg": "p:base",
    "root-bg": "p:yellow",
    "path": "p:yellow",
    "git": "p:lavender",
    "docker": "p:blue",
    "go": "p:mauve",
    "haskell": "p:mauve",
    "julia": "p:mauve",
    "lua": "p:mauve",
    "python": "p:mauve",
    "rust": "p:mauve",
    "react": "p:sapphire"
  },
  "transient_prompt": {
    "style": "plain",
    "foreground": "p:time",
    "template": "{{ now | date \"15:04:05\" }} {{ if gt .Code 0 }}<p:false>\u276f</>{{ else }}<p:true>\u276f</>{{ end }} ",
    "type": "text"
  },
  "secondary_prompt": {
    "style": "plain",
    "foreground": "p:rosewater",
    "template": "\u276f\u276f ",
    "type": "text"
  },
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:rosewater",
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "full"
          }
        },
        {
          "type": "docker",
          "style": "plain",
          "foreground": "p:docker",
          "template": "\uf308 {{ .Context }} "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:go",
          "template": "\ue627 {{ if not .Error }}{{ .Full }}{{ end }} "
        },
        {
          "type": "haskell",
          "style": "plain",
          "foreground": "p:haskell",
          "template": "\ue61f {{ if not .Error }}{{ .Full }}{{ end }} "
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "p:julia",
          "template": "\ue624 {{ if not .Error }}{{ .Full }}{{ end }} "
        },
        {
          "type": "lua",
          "style": "plain",
          "foreground": "p:lua",
          "template": "\ue620 {{ if not .Error }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:python",
          "template": "\ue73c {{ if not .Error }}{{ if .Venv }}venv/{{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_default": false,
            "display_mode": "context"
          }
        },
        {
          "type": "react",
          "style": "plain",
          "foreground": "p:react",
          "template": "îžº {{ if not .Error }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_default": false,
            "display_mode": "context"
          }
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "p:rust",
          "template": "\ue7a8 {{ if not .Error }}{{ .Full }}{{ end }} "
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        }
      ]
    },
    {
      "alignment": "right",
      "type": "prompt",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:true",
          "foreground_templates": ["{{ if .Code }}p:false{{ end }}"],
          "template": "\udb81\udd1f {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin"
          }
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "style": "plain",
          "foreground": "p:rosewater",
          "foreground_templates": ["{{ if gt .Code 0 }}p:false{{ end }}"],
          "template": "\u276f",
          "type": "text"
        }
      ]
    }
  ],

  "final_space": true,
  "version": 3
}
